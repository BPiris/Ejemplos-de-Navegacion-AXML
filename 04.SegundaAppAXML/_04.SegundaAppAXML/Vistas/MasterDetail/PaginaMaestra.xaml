<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="_04.SegundaAppAXML.Vistas.MasterDetail.PaginaMaestra"
             Title="Muestra de Paginas">              
  
  <StackLayout VerticalOptions="FillAndExpand">
    <ListView x:Name="listView" VerticalOptions="FillAndExpand" SeparatorVisibility="None">
      <ListView.ItemTemplate>
        <DataTemplate>
          <ImageCell Text="{Binding Titulo}"></ImageCell>
        </DataTemplate>
      </ListView.ItemTemplate>
    </ListView>
  </StackLayout>
  
</ContentPage>