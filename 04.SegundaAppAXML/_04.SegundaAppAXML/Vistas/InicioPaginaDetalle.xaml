    <masterDetail:PaginaMaestra x:Name="miPaginaMaestra"/>
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <vistas:Inicio></vistas:Inicio>
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>